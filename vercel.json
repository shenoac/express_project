{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts"
    }
  ]
}
